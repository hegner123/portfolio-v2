package templates

templ Hero() {
	<section class="hero-section" id="home" aria-labelledby="hero-name">
		<div class="hero-grid" aria-hidden="true">
			for range 500 {
				<div class="hero-grid-item"></div>
			}
		</div>
		<div class="hero-content">
			<h1 class="hero-name" id="hero-name">Michael Hegner</h1>
			<p class="hero-title">Full-Stack Engineer</p>
			<p class="hero-tagline">Building production AI agent systems and backend infrastructure.</p>
		</div>
	</section>
}
